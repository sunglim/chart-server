	"version": "0.2.0",
	"configurations": [
		{
			"name": "svc-insert RUN",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/cmd/csv-insert/csv-insert.go",
			"args": ["--csv-file-name", "MSFT2.csv",
